{
  "kind": "collectionType",
  "collectionName": "plants",
  "info": {
    "singularName": "plant",
    "pluralName": "plants",
    "displayName": "plant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "plants"
    },
    "price": {
      "type": "float"
    },
    "stock": {
      "type": "integer",
      "default": 0
    },
    "rating": {
      "type": "float",
      "default": 0
    },
    "enableSizeOptions": {
      "type": "boolean",
      "default": false
    },
    "priceSmall": {
      "type": "float"
    },
    "priceMedium": {
      "type": "float"
    },
    "priceLarge": {
      "type": "float"
    },
    "discountPercent": {
      "type": "float"
    },
    "offerStart": {
      "type": "date"
    },
    "offerEnd": {
      "type": "date"
    },
    "extraOfferLabel": {
      "type": "string",
      "description": "Extra offer tag on plant card (e.g. 'Extra 10% off', 'Buy 1 Get 1'). Shown next to 20% off when set."
    },
    "cart": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cart.cart",
      "mappedBy": "plant"
    }
  }
}
